From: Roman Stratiienko <r.stratiienko@gmail.com>
Date: Mon, 6 Nov 2023 04:00:49 +0200
Subject: [PATCH 1/2] Rename Android.mk to avoid interfering with AOSPext

Signed-off-by: Roman Stratiienko <r.stratiienko@gmail.com>
---
 Android.mk => Android.mk_ | 5 ++---
 1 file changed, 2 insertions(+), 3 deletions(-)
 rename Android.mk => Android.mk_ (99%)

diff --git a/Android.mk b/Android.mk_
similarity index 99%
rename from Android.mk
rename to Android.mk_
index c32afbeb3..d02efae5b 100644
--- a/Android.mk
+++ b/Android.mk_
@@ -287,10 +287,9 @@ LOCAL_SRC_FILES := \
 
 LOCAL_STATIC_LIBRARIES := \
         libflatbuffers-framework \
-        arm_compute_library
 
 LOCAL_SHARED_LIBRARIES := \
-        liblog
+        liblog libarm_compute
 
 LOCAL_CFLAGS := \
         -std=$(CPP_VERSION) \
@@ -511,7 +510,6 @@ endif
 LOCAL_STATIC_LIBRARIES := \
         libneuralnetworks_common \
         libflatbuffers-framework \
-        arm_compute_library \
         $(ARMNN_BACKEND_TEST_STATIC_LIBRARIES)
 
 LOCAL_WHOLE_STATIC_LIBRARIES := libarmnn
@@ -526,6 +524,7 @@ LOCAL_SHARED_LIBRARIES := \
         android.hardware.neuralnetworks@1.0 \
         android.hidl.allocator@1.0 \
         android.hidl.memory@1.0 \
+        libarm_compute \
         $(ARMNN_BACKEND_TEST_SHARED_LIBRARIES)
 
 ifeq ($(ARMNN_INCLUDE_LIBOPENCL),1)
